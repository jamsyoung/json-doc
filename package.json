{
    "name": "json-doc",
    "version": "0.1.0",
    "author": "James Young <jmeyoung@gmail.com> (http://jamsyoung.com)",
    "description": "Outputs an interactive html file like package.json.nodejitsu.com",
    "main": "json-doc.js",
    "bin": "./json-doc.js",
    "scripts": {
        "build-man": "mkdir -p man/man1/; node_modules/.bin/marked-man src/man/man1/json-doc.1.md > man/man1/json-doc.1",
        "postinstall": "./src/build/postinstall.sh"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jamsyoung/json-doc"
    },
    "bugs": {
        "url": "https://github.com/jamsyoung/json-doc/issues",
        "email": "jmeyoung@gmail.com"
    },
    "man": "./man/man1/json-doc.1",
    "devDependencies": {
        "marked-man": "0.0.2"
    },
    "engines": {
        "node": ">=0.10.15",
        "npm": ">=1.3.5"
    },
    "keywords": [
        "docs",
        "documentation",
        "json",
        "json-schema",
        "schema"
    ],
    "license": {
        "type": "MIT",
        "url": "https://github.com/jamsyoung/json-doc/blob/master/license.md"
    }
}
